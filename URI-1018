URI-1018

#include <iostream>
using namespace std;

int main()
{
    int q,ma;
    int arr[7]={100,50,20,10,5,2,1};
    cin >> ma ;
    cout << ma << endl;
        for(int i = 0 ; i < 7 ;i++)
            {

            q = ma/arr[i];
            ma %=arr[i] ;
            cout << q << " nota(s) de R$ " << arr[i] << ",00" <<endl;
        }

    return 0;
}
