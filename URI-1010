#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    int id1,id2,num1,num2;
    float p1,p2;
   while(cin >> id1 >> num1 >>p1 )
   {
       cin >> id2 >> num2 >> p2 ;
       cout << "VALOR A PAGAR: R$ "<<fixed << setprecision(2)<<num1*p1+num2*p2 <<endl;

   }

    return 0;
}
